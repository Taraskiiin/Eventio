  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "npm run _eslint & npm run _stylelint && wait",
    "lint:fix": "npm run _prettier && npm run _eslint -- --fix && npm run _stylelint -- --fix",
    "_prettier": "prettier --write \"./src/\"",
    "_eslint": "eslint  \"./src/\" -c .eslintrc --ext .ts,.tsx  --max-warnings=0",
    "_stylelint": "stylelint \"./src/**/*.{css,scss,module.scss}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/google.maps": "^3.46.1",
    "@types/react": "^17.0.35",
    "@types/react-router-dom": "^5.1.9",
    "@types/react-slick": "^0.23.6",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-staged": "^1.0.1",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix --max-warnings=0"
    ],
    "*.{html,md,mdx}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write",
      "stylelint --fix --syntax scss"
    ]
  }
}
